	"version": "0.2.0",
	"configurations": [
		{
			"name": "Preview (Chrome)",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}/dist",
			"preLaunchTask": "start",
			"runtimeArgs": [
				"--enable-unsafe-es3-apis"
			],
			"sourceMaps": true,
		},
		{
			"name": "Preview (Firefox)",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"preferences": {
				"sourcemaps": true
			}
		},
		{
			"name": "Preview (Edge)",
			"type": "msedge",
			"request": "launch",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceRoot}/dist",
			"preLaunchTask": "start",
			"runtimeArgs": [
				"--enable-unsafe-es3-apis"
			],
			"sourceMaps": true,
		},
	]
}
